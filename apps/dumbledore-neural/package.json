{
  "name": "@hp/dumbledore",
  "version": "0.1.0",
  "main": "./dist/node_modules/index.js",
  "typings": "./dist/node_modules/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "build:watch": "yarn build --watch"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": true,
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "test\\.ts"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "nodemon": "^1.18.5",
    "ts-jest": "^23.10.4"
  },
  "dependencies": {
    "@types/uuid": "^3.4.4",
    "lodash": "^4.17.11",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "uuid": "^3.3.2"
  }
}